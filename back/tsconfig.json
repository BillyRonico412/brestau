{
	"compilerOptions": {
		"strict": true,
		"skipLibCheck": true,
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"noEmit": true,
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"baseUrl": ".",
		"paths": {
			"@back/*": ["./src/*"]
		}
	},
	"exclude": ["src/generated"]
}
